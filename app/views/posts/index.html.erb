
<div class="main-container">
<h2>
  <%= link_to 'Add New', new_post_path, class: 'header-menu' %>
  <div class="pagetitle">
  My Posts
  </div>
</h2>
<ul class="list-unstyled">
<% @posts.each do |post| %>
<li>
<%= link_to post.title, post_path(post) %>
<%= link_to '[Edit]', edit_post_path(post), class: 'command' %>
<%= link_to '[x]', 
  post_path(post),
  method: :delete,
  class: 'command',
  data: { confirm: 'Sure?' } %>
</li>
<% end %>
</ul>
</div>
