<!DOCTYPE html>
<html lang="en">

<div class="main-container">
  <div class="row">
    <div class="col-md-6">
      <div class="row">
        <div class="col-md-6">
          <h4>自分の投稿</h4>
        </div>
        <div class="col-md-6">
          <%= link_to '新規投稿', new_post_path, class: 'addnew_btn btn btn-primary' %>
        </div>
      </div>
      <div class="row"> 
    <div class="col-md-12">
        <% @posts.each do |post| %>
          <%= link_to post.title, post_path(post) %>
          <%= link_to 'Edit', edit_post_path(post), class: 'command btn btn-primary' %>
          <%= link_to 'x',
              post_path(post),
              method: :delete,
              class: 'command btn btn-primary',
              data: { confirm: 'Sure?' } %>
          <% end %>
    </div>
    <div class="col-md-12">
      test
    </div>
